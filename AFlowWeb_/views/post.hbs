{{!< default}}
{{#post}}
    <div class="post">
        <h1 class="post-title">
            {{title}}
        </h1>
        <div class="post-meta">
            {{date format="MMMM DD , YYYY"}}
        </div>
        <div class="post-content">
            {{content}}
        </div>
    </div>
{{/post}}
